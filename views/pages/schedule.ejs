<div id="schedule" class="mt-5">
    <% if(score.simalateEmptySchedule === "true"){ %>
        <p class="section-subtitle text-center">
            Programul va fi afișat după tragerea la sorț
        </p>
      <% } else{ %>
        <p class="section-subtitle text-center">
          Programul este estimativ având în vedere că unele meciuri pot avea două sau trei seturi 
        </p>
        <% displayScore=false %>
        <%- include('ranking'); -%>
        <%- include('fixtures'); -%>
     <% } %>
</div>
